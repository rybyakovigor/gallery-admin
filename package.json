{
  "name": "gallery-admin",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite & tcm --watch src",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check:all": "concurrently \"npm:check:*(!all)\"",
    "check:all:pre-commit": "concurrently -r \"npx lint-staged --relative\" \"npm:check:types\"",
    "check:lint": "eslint src",
    "check:prettier": "prettier --check src",
    "check:stylelint": "stylelint src/**/*.css",
    "check:types": "tsc --noEmit",
    "fix:all": "concurrently \"npm:fix:*(!all)\"",
    "fix:lint": "eslint src --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "stylelint src/**/*.css --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "5.3.1",
    "antd": "5.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "typed-css-modules": "0.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-sonarjs": "0.24.0",
    "husky": "8.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-recess-order": "5.0.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "typescript": "5.2.2",
    "vite": "5.1.4"
  }
}
