{
  "name": "gallery-admin",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite & tcm --watch src",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check:all": "concurrently \"npm:check:*(!all)\"",
    "check:all:pre-commit": "concurrently -r \"npx lint-staged --relative\" \"npm:check:types\"",
    "check:lint": "eslint src",
    "check:prettier": "prettier --check src",
    "check:stylelint": "stylelint src/**/*.css",
    "check:types": "tsc --noEmit",
    "fix:all": "concurrently \"npm:fix:*(!all)\"",
    "fix:lint": "eslint src --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "stylelint src/**/*.css --fix"
  },
  "dependencies": {
    "@ant-design/icons": "6.0.0",
    "antd": "5.26.5",
    "axios": "1.10.0",
    "mobx": "6.13.7",
    "mobx-react-lite": "4.1.0",
    "primeflex": "4.0.0",
    "primeicons": "7.0.0",
    "primereact": "10.9.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.7.0",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "@vitejs/plugin-react": "4.7.0",
    "concurrently": "9.2.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "stylelint": "16.22.0",
    "stylelint-config-recess-order": "7.1.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "7.0.0",
    "typed-css-modules": "0.9.1",
    "typescript": "5.8.3",
    "vite": "7.0.5"
  }
}
